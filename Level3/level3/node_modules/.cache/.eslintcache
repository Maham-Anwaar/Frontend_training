st, but if it already exists then it just increments amount :D\n            let changed = 0;\n            state.map(item=>{\n                if(item.name===action.payload.name){\n                    item.amount+=1;\n                    changed = 1;\n                }\n            })\n            if(changed) return [...state]\n            return [...state, action.payload]\n            \n        }\n    }\n}\n\nexport default listReducer\n\n\n","/Users/mappletechnologies/Desktop/Frontend_Training/Level_3/level3/src/myRedux/actions.js",[],[],{"ruleId":"234","severity":1,"message":"235","line":8,"column":13,"nodeType":"236","endLine":8,"endColumn":62},{"ruleId":"234","severity":1,"message":"235","line":11,"column":21,"nodeType":"236","endLine":11,"endColumn":42},{"ruleId":"234","severity":1,"message":"235","line":9,"column":13,"nodeType":"236","endLine":9,"endColumn":62},{"ruleId":"237","severity":1,"message":"238","line":75,"column":31,"nodeType":"239","messageId":"240","endLine":75,"endColumn":33},{"ruleId":"241","severity":1,"message":"242","line":88,"column":9,"nodeType":"243","endLine":88,"endColumn":20,"suggestions":"244"},{"ruleId":"245","severity":1,"message":"246","line":7,"column":19,"nodeType":"247","messageId":"248","endLine":7,"endColumn":27},{"ruleId":"237","severity":1,"message":"238","line":11,"column":25,"nodeType":"239","messageId":"240","endLine":11,"endColumn":27},{"ruleId":"241","severity":1,"message":"249","line":18,"column":8,"nodeType":"243","endLine":18,"endColumn":17,"suggestions":"250"},{"ruleId":"237","severity":1,"message":"238","line":21,"column":25,"nodeType":"239","messageId":"240","endLine":21,"endColumn":27},{"ruleId":"251","severity":1,"message":"252","line":28,"column":23,"nodeType":"253","messageId":"254","endLine":28,"endColumn":25},{"ruleId":"245","severity":1,"message":"255","line":1,"column":16,"nodeType":"247","messageId":"248","endLine":1,"endColumn":24},{"ruleId":"234","severity":1,"message":"235","line":12,"column":17,"nodeTyp